{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": "tsc",
    "typeCheck": false,
    "deleteOutDir": true,
    "assets": [
      {
        "include": "modules/**/views/*.hbs",
        "outDir": "dist"
      }
    ]
  }
}
